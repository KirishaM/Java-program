import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
      int age = sc.nextInt();
      int dist = sc.nextInt();
      float fare = dist*5;
      if(age < 12){
        System.out.println((int)(fare*0.5));
      }else if(age>=60){
        System.out.println((int)(fare*0.75));
      }else{
        System.out.println((int)fare);
      }
  }
}
